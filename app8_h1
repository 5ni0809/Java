//app8_h1 費氏數列
class Fib
{
	long  fib1(int i)
	{            
	    if(i==0 || i==1)return i;
	    return fib1(i-1)+fib1(i-2);
	}
}
public class app8_h1
{
	public static void main(String args[])
	{
		Fib f= new Fib();
		for(int i=0;i<40;i++)
		{
			System.out.println("f"+i+" = "+f.fib1(i));
		}	
        }
}
