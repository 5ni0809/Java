//app7_9, 簡單的遞迴函數
public class app7_9
{
       public static void main(String args[])
       { 
              System.out.println("費氏數列 f(0)=1,f(1)=1,...");
              for(int i=0;i<6;i++)
	           System.out.println("f("+i+")="+fib(i));
       }
       public static int fib(int n)
       { 
             if(n==0||n==1) return 1;
             return (fib(n-1)+fib(n-2));
       }
}
